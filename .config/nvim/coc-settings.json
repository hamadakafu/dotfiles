{
  "semanticTokens.enable": true,
  "snippets.extends": {
    "latex": [
      "tex"
    ],
    "zsh": [
      "sh"
    ],
    "bash": [
      "sh"
    ]
  },
  "snippets.ultisnips.directories": [
    "~/.config/nvim/plugins/UltiSnips"
  ],
  "coc.preferences.enableMessageDialog": true,
  "suggest.autoTrigger": "always",
  "suggest.noselect": true,
  "suggest.timeout": 15000,
  "python.formatting.provider": "black",
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "go.checkForUpdates": "ask",
  "texlab.latexFormatter": "latexindent",
  "languageserver": {
    "zig": {
      "command": "/home/kafuhamada/mybin/zls",
      "filetypes": [
        "zig"
      ]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": [
        "haskell",
        "lhaskell"
      ]
    },
    "hdlChecker": {
      "command": "hdl_checker",
      "args": [
        "--lsp"
      ],
      "filetypes": [
        "vhdl",
        "verilog",
        "systemverilog"
      ]
    }
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "Lua.telemetry.enable": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.3/clangd_15.0.3/bin/clangd",
  "clangd.checkUpdates": true
}
