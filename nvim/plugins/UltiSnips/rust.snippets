snippet mypow "バイナリ法 binary" b
fn mypow(a: usize, mut b: usize, mmm: usize) -> usize {
    let mut ans = 1;
    let mut ppp = a;
    while b > 0 {
        if b % 2 == 1 {
            ans *= ppp;
            ans %= mmm;
        }
        ppp *= ppp;
        ppp %= mmm;

        b /= 2;
    }
    ans
}
